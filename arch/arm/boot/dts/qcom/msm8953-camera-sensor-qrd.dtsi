		qcom,cam-vreg-name = "cam_vdig", "cam_vio", "cam_vana",
							"cam_vaf";
		qcom,cam-vreg-min-voltage = <1200000 1800000 2800000 0>;
		qcom,cam-vreg-max-voltage = <1200000 1800000 2800000 0>;
		qcom,cam-vreg-op-mode = <200000 200000 80000 0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_default
